{
  "compilerOptions": {
    "target": "es6",
    "module": "ESNext",
    "strict": true,
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "noEmit": true,
    "isolatedModules": true,
    
  },
  
  "include": ["src/**/*.tsx", "src/**/*.ts", "src/components/Header.js"],
  "exclude": ["node_modules"]
}